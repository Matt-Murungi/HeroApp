ssage = JSON.stringify(this.hero, null, '\\n');\n    },\n    clearPower() {\n      this.hero.power = '';\n    },\n  },\n};\n",{"version":3,"sources":["heroes.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6HA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"heroes.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"content-container\">\n    <div class=\"section content-title-group\">\n      <h2 class=\"title\">Heroes</h2>\n    </div>\n    <div class=\"columns\">\n      <div class=\"column is-8\">\n        <div class=\"card edit-detail\">\n          <header class=\"card-header\">\n            <p class=\"card-header-title\">{{ hero.firstName }}</p>\n          </header>\n          <div class=\"card-content\">\n            <div class=\"content\">\n              <div class=\"field\">\n                <label class=\"label\" for=\"id\">{{ hero.id }}</label>\n                <label class=\"input\" id=\"id\" readonly></label>\n              </div>\n              <div class=\"field\">\n                <label class=\"label\" for=\"firstName\">first name</label>\n                <input class=\"input\" id=\"firstName\" />\n              </div>\n              <div class=\"field\">\n                <label class=\"label\" for=\"lastName\">last name</label>\n                <input class=\"input\" id=\"lastName\" />\n              </div>\n              <div class=\"field\">\n                <label class=\"label\" for=\"description\">description</label>\n                <textarea\n                  class=\"input\"\n                  id=\"description\"\n                  type=\"text\"\n                  v-model=\"hero.description\"\n                />\n              </div>\n              <div class=\"field\">\n                <label class=\"label\">cape color</label>\n                <label class=\"radio\" for=\"color-red\">\n                  <input\n                    type=\"radio\"\n                    id=\"color-red\"\n                    value=\"red\"\n                    v-model=\"hero.capeColor\"\n                  />\n                  red\n                </label>\n                <label class=\"radio\" for=\"color-blue\">\n                  <input\n                    type=\"radio\"\n                    id=\"color-blue\"\n                    value=\"blue\"\n                    v-model=\"hero.capeColor\"\n                  />\n                  blue\n                </label>\n                <label class=\"radio\" for=\"color-green\">\n                  <input\n                    type=\"radio\"\n                    id=\"color-green\"\n                    value=\"green\"\n                    v-model=\"hero.capeColor\"\n                  />\n                  green\n                </label>\n                <div\n                  class=\"color-line\"\n                  :style=\"{ 'background-color': hero.capeColor }\"\n                ></div>\n              </div>\n              <div class=\"field\">\n                <label for=\"power\">\n                  super power\n                  <div class=\"select is-primary\">\n                    <select\n                      id=\"power\"\n                      v-model=\"hero.power\"\n                      :class=\"{ invalid: !hero.power }\"\n                      @keyup.esc=\"clearPower\"\n                    >\n                      <option disabled value>Please select one</option>\n                      <option>Speed</option>\n                      <option>Flight</option>\n                      <option>Strength</option>\n                      <option>Invisibility</option>\n                    </select>\n                  </div>\n                </label>\n              </div>\n              <div class=\"field\">\n                <label class=\"checkbox\" for=\"active\">\n                  active\n                  <input\n                    type=\"checkbox\"\n                    class=\"is-primary\"\n                    id=\"active\"\n                    v-model=\"hero.active\"\n                  />\n                </label>\n              </div>\n            </div>\n          </div>\n          <footer class=\"card-footer\">\n            <button\n              class=\"link card-footer-item cancel-button\"\n              @click=\"cancelHero\"\n            >\n              <i class=\"fas fa-undo\"></i>\n              <span>Cancel</span>\n            </button>\n            <button class=\"link card-footer-item\" @click=\"saveHero\">\n              <i class=\"fas fa-save\"></i>\n              <span>Save</span>\n            </button>\n          </footer>\n        </div>\n        <div class=\"notification is-info\">\n          <pre>\n            {{ message }}\n          </pre>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Heroes',\n  data() {\n    return {\n      hero: {\n        id: 20,\n        firstName: 'Funke',\n        lastName: 'Bird',\n        description: 'the cat whisperer',\n        capeColor: '',\n        power: '',\n        active: true,\n      },\n      message: 'Vuejs be awesome',\n    };\n  },\n  methods: {\n    cancelHero() {\n      this.message = '';\n    },\n    saveHero() {\n      this.message = JSON.stringify(this.hero, null, '\\n');\n    },\n    clearPower() {\n      this.hero.power = '';\n    },\n  },\n};\n</script>\n"]}]}